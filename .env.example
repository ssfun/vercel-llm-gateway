# =================== 基础配置 ===================
# CORS 允许的源
ALLOWED_ORIGIN=*

# 默认超时时间（毫秒）
DEFAULT_TIMEOUT=20000

# Background Function 超时时间（毫秒）
BACKGROUND_TIMEOUT=240000

# 最大响应大小（字节）
MAX_RESPONSE_SIZE=6291456

# 是否启用重试
ENABLE_RETRY=true

# 是否启用降级（Background 失败时降级到 Edge）
ENABLE_FALLBACK=true

# 日志级别：DEBUG | INFO | WARN | ERROR
LOG_LEVEL=INFO

# =================== 自定义代理配置 ===================
# JSON 格式的自定义代理配置
# PROXY_CONFIG='{"custom-api": {"host": "api.custom.com", "timeout": 30000, "maxRetries": 3}}'

# =================== API Keys（可选）===================
# 如果你需要在网关层添加 API Keys
# OPENAI_API_KEY=sk-...
# ANTHROPIC_API_KEY=sk-ant-...
# GOOGLE_API_KEY=...
